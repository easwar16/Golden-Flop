{
  "expo": {
    "name": "GoldenFlop",
    "slug": "goldenflop",
    "version": "1.0.0",
    "owner": "goldenflop.skr",
    "extra": {
      "eas": {
        "projectId": "49ec6e85-c4af-4819-abf6-94c7c8f181b9"
      }
    },
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.goldenflop.skr.goldenflop"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1A1025",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.goldenflop.skr.goldenflop"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-av",
      "expo-audio",
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "2.1.0"
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "cover",
          "backgroundColor": "#9858B9",
          "dark": {
            "backgroundColor": "#9858B9"
          }
        }
      ],
      "expo-font",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
